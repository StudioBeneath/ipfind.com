@font-face {
	font-family: 'FF Mark Web';
	font-style: normal;
	font-weight: 400;
	src: url('../fonts/MarkPro-Book.woff') format('woff'),
       url('../fonts/MarkPro-Book.ttf') format('truetype');
}

@font-face {
	font-family: 'FF Mark Web';
	font-style: normal;
	font-weight: 700;
	src: url('../fonts/MarkPro-Bold.woff') format('woff'),
       url('../fonts/MarkPro-Bold.ttf') format('truetype');
}
